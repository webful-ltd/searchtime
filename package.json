{
  "name": "SearchTime",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "npm run lint && npm run test-sans-lint",
    "test-sans-lint": "jest",
    "lint": "eslint app __tests__"
  },
  "eslintConfig": {
    "plugins": [
      "react",
      "react-native"
    ],
    "extends": "eslint-config-airbnb",
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "no-extra-semi": 2,
      "react/jsx-filename-extension": 0
    }
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-native": "0.39.2",
    "react-native-button": "1.7.1",
    "react-native-datepicker": "1.4.1",
    "react-native-simple-store": "^1.1.0",
    "react-redux": "5.0.1",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "eslint-plugin-react-native": "2.2.1",
    "expect": "1.20.2",
    "jest": "^18.1.0",
    "react-test-renderer": "^15.4.2"
  }
}
